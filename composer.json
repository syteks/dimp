{
    "name": "syteks/dimp",
    "description": "This helps import data and transform it into a array in order to manipulated. It includes a more complexe way of importing where we can set different kinds of validation rules when we import the data.",
    "type": "library",
    "license": "MIT",
    "keywords": [""],
    "authors": [
        {
            "name": "Sergiu Grigore Turcanu",
            "email": "sergiugrigoreturcanu@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Syteks\\Dimp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.34",
        "phpunit/phpunit": "^10.3"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
